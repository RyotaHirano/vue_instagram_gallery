{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "jest"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true,
    "mocha": true,
    "jest/globals": true
  },
  "globals": {
    "google": true
  },
  "rules": {
    "no-var": "off",
    "prefer-const": "error",
    "no-console": "warn",
    "no-unused-vars": "off"
  }
}