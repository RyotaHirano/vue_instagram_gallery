{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "jest": true,
    "google": true
  },
  "rules": {
    "no-var": 2,
    "prefer-const": 2,
    "no-console": 1
  }
}